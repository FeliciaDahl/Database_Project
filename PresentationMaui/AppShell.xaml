<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PresentationMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PresentationMaui"
    xmlns:pages="clr-namespace:PresentationMaui.Pages"
    Shell.FlyoutBehavior="Disabled"
    Shell.NavBarIsVisible="False"
    Title="PresentationMaui">

  
    <ShellContent Title="ListPage" Route="ListPage" ContentTemplate="{DataTemplate pages:ListPage}"  />
    <ShellContent Title="Add" Route="AddPage" ContentTemplate="{DataTemplate pages:AddPage}"  />
    <ShellContent Title="Edit" Route="EditPage" ContentTemplate="{DataTemplate pages:EditPage}"  />
    
</Shell>
